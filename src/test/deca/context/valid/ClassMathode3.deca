class Point2D {
    int x;
    int y;
    
    void diag(int a) {
    x = x + a;
    y = y + a;
    }
}


class Point3D extends Point2D {
    int z; 
    void diag(int a) {
    x = x + a;
    y = y + a;
    z = z + a;
    }
}