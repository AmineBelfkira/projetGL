class SuiteFibonacci{  
  
    int a=0;
    int b=1;  
    int c=1; 
     
    void Fibonacci(int c){    
       if(c > 0){    
            c = a+b;    
            a=b;    
            b=c;       
            Fibonacci(c-1);    
        }    
    }

}
